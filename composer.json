{
    "name": "pkeogan/laravel-permission",
    "description": "Permission System for Laravel 5.5 and Up",
    "version": "1.0",
    "keywords": [
        "pkeogan",
        "laravel",
        "permission",
        "security"
    ],
    "homepage": "https://github.com/pkeogan/laravel-permission",
    "license": "MIT",
    "authors": [
        {
            "name": "Peter Keogan",
            "email": "peter@pkeogan.com",
            "homepage": "https://pkeogan.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.0",
        "illuminate/auth": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/container": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/contracts": "~5.3.0|~5.4.0|~5.5.0|~5.6.0",
        "illuminate/database": "~5.4.0|~5.5.0|~5.6.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.3.0|~3.4.2|^3.5.0",
        "phpunit/phpunit" : "^5.7|6.2|^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pkeogan\\Permission\\": "src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pkeogan\\Permission\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkeogan\\Permission\\PermissionServiceProvider"
            ]
        }
    }
}
